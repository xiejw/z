# This is the step to prepare the env to play.
use python3.12 on macOs
python3.12 -m venv .build
source .build/bin/activate
mkdir .build/weights
wget -O .build/weights/state.tar.gz https://github.com/xiejw/z/releases/download/v0.0.1/c4-resnet.pt.tar.gz
tar  -C .build/weights -xvf .build/weights/state.tar.gz
python configure.py --model_dir .build/weights/
make play

